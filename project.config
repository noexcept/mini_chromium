[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group crashpad-owners
[access "refs/heads/*"]
	create = group crashpad-owners
	push = group crashpad-owners
	push = group crashpad-committers
	read = group Anonymous Users
	submit = group crashpad-owners
	submit = group crashpad-committers
[label "Commit-Queue"]
[label "Verified"]
[notify "crashpad-dev"]
	email = crashpad-dev@chromium.org
	type = all_comments
	type = submitted_changes
	header = cc
[submit]
	action = cherry pick
